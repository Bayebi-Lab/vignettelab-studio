{
  "buildCommand": "vite build",
  "outputDirectory": "dist",
  "functions": {
    "api/create-payment-intent.ts": {
      "maxDuration": 60,
      "memory": 1024,
      "regions": ["sfo1"]
    }
  },
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/$1" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
